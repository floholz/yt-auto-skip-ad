{
  "name": "YT auto skip Ads",
  "description": "Automatically skips ads on Youtube, when possible ;)",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "icons": {
    "16": "/images/logo-16.png",
    "32": "/images/logo-32.png",
    "48": "/images/logo-48.png",
    "128": "/images/logo-128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
